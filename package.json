{
  "name": "react-freeform",
  "version": "0.5.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src --ext .jsx --ext .js",
    "clean:lib": "rimraf ./HOC/* && rimraf ./components/*",
    "compile:lib": "babel src --out-dir .",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "build": "npm run clean:lib && npm run compile:lib",
    "test": "NODE_ENV=test nyc npm run test:raw",
    "test:raw": "NODE_ENV=test mocha --require @babel/register --require test/test_helper.js --recursive test/**/*.{js,jsx}",
    "test:single": "NODE_ENV=test mocha --require @babel/register --require test/test_helper.js --recursive",
    "preversion": "npm run build && npm run test",
    "postpublish": "npm run docs:deploy",
    "docs:dev": "npx styleguidist server",
    "docs:build": "npx styleguidist build",
    "docs:deploy": "npm run docs:build && gh-pages --dist styleguide"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpenserJ/react-freeform.git"
  },
  "author": "Spenser Jones <hello@spenserj.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SpenserJ/react-freeform/issues"
  },
  "homepage": "http://spenserj.com/react-freeform/",
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/register": "^7.6.0",
    "@vizworx/babel-preset": "^1.0.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-istanbul": "^5.2.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "chai": "^4.2.0",
    "codecov": "^3.5.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "gh-pages": "^2.1.1",
    "jsdom": "^15.1.1",
    "library-utils": "^2.4.1",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-styleguidist": "^9.1.16",
    "rimraf": "^3.0.0",
    "sinon": "^7.4.2",
    "webpack": "^4.40.2"
  },
  "peerDependencies": {
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  },
  "dependencies": {
    "invariant": "^2.2.4",
    "object-path": "^0.11.4",
    "object-path-immutable": "^3.1.1",
    "prop-types": "^15.7.2"
  },
  "nyc": {
    "require": [
      "@babel/register",
      "./test/test_helper.js"
    ],
    "exclude": [
      "HOC",
      "components",
      "styleguide"
    ],
    "include": [
      "src/**/*.{js,jsx}"
    ],
    "extension": [
      ".jsx"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "sourceMap": false,
    "instrument": false,
    "cache": true,
    "all": true
  }
}
